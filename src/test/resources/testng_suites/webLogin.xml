<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="phptravels.net tests" parallel="tests" thread-count="6">
	<parameter name="suiteOwner" value="qpsdemo"/>
	<parameter name="jenkinsJobName" value="WEB-DRIVER-TESTS"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="jenkinsRegressionPipeline" value="DEMO"/>
    <parameter name="jenkinsRegressionMatrix_full_regression" value="browser: chrome;"/>
	<parameter name="jenkinsAutoScreenshot" value="true"/>
	<test name="Search, fillForm and login tests">
		<classes>
			<class name="net.phptravels.WebLogin" />
		</classes>
	</test>
</suite>